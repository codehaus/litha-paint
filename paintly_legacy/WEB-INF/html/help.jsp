<%@ page import="java.util.*" %><%@ page contentType="text/html" %><html>	<head>		<link rel="stylesheet" type="text/css" href="./css/style.css">		<title>LiTha-Paint help-subsystem,web-based drawing service,2006</title>	</head><body bgcolor="#FFFFFF"><%	String[] itemChapter	= null;		String[][] helpTree =	{{"root",	"Help system",				"help_root.html",		null},							{"basics",			"Litha-Paint Basics",	"help_basics.html",		"root"},														{"b_nodes",			"Nodes",					   "help_nodes.html",		"basics"},							{"b_c_menu",		"Context Menu",			"help_c_menu.html",		"basics"},							{"b_draw_exit",	"Exit Polygon, polyline and Bezier drawing mode by pressing ESC",	"help_draw_exit.html",		"basics"},														{"drawing",			"Drawing",					"help_drawing.html",    "root"},														{"d_bez_curves",	"Bezier Curves",			"help_bez_curves.html",	"drawing"},														{"d_prc_drawing",	"Proceeding with drawing Bezier curve objects",	"help_prc_drawing.html",	"drawing"},														{"d_drg_nodes",	"Dragging Nodes",		   "help_drg_nodes.html",	"drawing"},							{"d_splitting",	"Splitting",				"help_splitting.html",	"drawing"},							{"d_joining",		"Joining",					"help_joining.html",	   "drawing"},							{"d_rotating",		"Rotating",					"help_rotating.html",	"drawing"},							{"d_moving",		"Moving",					"help_moving.html",		"drawing"},							{"d_skewing",		"Skewing",					"help_skewing.html",	   "drawing"},							{"d_closing",		"Closing",					"help_closing.html",	   "drawing"},							{"d_filling",		"Filling",					"help_filling.html",	   "drawing"},							{"d_typing_text", "Typing Text",          "help_typing_text.html","drawing"},							{"d_ch_stroke",   "Changing stroke",      "help_changing_stroke.html","drawing"},							{"d_copypaste",   "Copy/Paste",           "help_copypaste.html",  "drawing"},							{"d_layers",      "Layers",               "help_layers.html",     "drawing"},																					{"documents",     "Working with documents","help_documents.html", "root"},							{"dd_importing",  "Importing Image",      "help_importing.html",  "documents"},							{"dd_publishing", "Publishing Image in the Web",  "help_publishing.html",  "documents"},							{"dd_downloading", "Downloading Image to the Desktop",  "help_image_downloading.html",  "documents"},							{"dd_exporting",  "Exporting Image to the Desktop",  "help_exporting.html",  "documents"},							{"dd_openSave",  "Open/Save Document in your Remote Home Directory",      "help_OpenSave.html",   "documents"}};							/*	help main parts		- features		- file		- open		- save		- export		- upload		- publish			- drawing (hot keys & etc.)		- tools		- ellipse		- ...		- about		- about service		- about us		- beta-testers		- donations		*/								Hashtable chapterTable = new Hashtable();		for(int i = 0; i < helpTree.length; i++){		chapterTable.put(helpTree[i][0], helpTree[i]);	}		if(request.getParameter("ch") != null){			itemChapter = (String [])chapterTable.get((String)request.getParameter("ch").trim());	}		if(request.getParameter("ch") == null || itemChapter == null){		itemChapter = helpTree[0];	}	%><table width="95%" cellpadding="0" cellspacing="0" border="0" align="center" style="margin:5">	<tr>		<td width="350" class="systemLogoFont" height="23">			<!-- logo -->			<img src="./img/litha/logo.gif" height="20px">&nbsp;Litha - Paint&nbsp;(Beta)		</td>		<!--td>&nbsp;</td>	</tr>	<tr-->		<td height="20" colspan="2" valign="middle" align="left" style="padding-top:10">&nbsp;</td>	</tr></table><hr color="#42C419" align="center" width="95%" style="margin:1"/><table border="0" width="95%" align="center" style="margin-top:-5">	<tr>		<td></td>		<td>			<%				String[] itemStrArray = itemChapter;								if(itemChapter[3] != null){							String linkStr = "";										do{						itemStrArray = (String[])chapterTable.get(itemStrArray[3]);												linkStr = "<span class='systemGreenFont'>&raquo;</span>&nbsp;<a class='help_link' href='./help.jsp?ch=" + itemStrArray[0] + "'>" + itemStrArray[1] + "</a>&nbsp&nbsp;" + linkStr;					}while(itemStrArray[3] != null);					out.print(linkStr);				}				%>			<br><br>		</td>	</tr>	<tr>			<td width="30%" valign="top">			<!-- help menu -->			<table width="100%" cellpadding="0" cellspacing="0" >					<%						Enumeration enumHelp = chapterTable.elements();						while (enumHelp.hasMoreElements()) {							itemStrArray = (String[]) enumHelp.nextElement();							if(itemStrArray[3] != null && itemStrArray[3].equals("root")){								out.print("<tr><td align='left'>");								out.print("<li><a class='help_link' href='./help.jsp?ch=" + itemStrArray[0] + "'>" + itemStrArray[1] + "</a></li>");								out.print("</td></tr>");							}							}					%>			</table>					</td>		<td valign="top" height="400" class="help_text" style="margin:10">			<jsp:include page="<%= "help/"+itemChapter[2] %>" flush="true" />				</td>	</tr>		</table><div id="copyright" class="helpFont" align="center">©2006-2007&nbsp;Litha-Soft&nbsp;<a class='help_link' href="http://litha-paint.com">Litha-Paint</a></div><script>
   var screenW = 640, screenH = 480;

   if (parseInt(navigator.appVersion)>3) {
      screenW = screen.width;
      screenH = screen.height;
   }
   </script></body>